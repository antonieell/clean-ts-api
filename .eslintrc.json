{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb/base",
    "airbnb-typescript/base",
    "plugin:prettier/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "prettier/prettier": ["error"],
    "@typescript-eslint/quotes": ["error", "double"]
  }
}
